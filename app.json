{
    "name": "TheBrownPhone",
    "description": "A phone for Mr. Brown",
    "repository": "https://github.com/ashleylovespizza/brownphone",
    "logo": "https://www.dropbox.com/s/nnu4vc495ykxnm9/brownphone.png?dl=1",
    "addons": ["heroku-postgresql:hobby-dev"],
    "env": {
        "APP_DEBUG": "true",
        "APP_ENV": "production",
        "DB_CONNECTION": "pgsql",
        "S3_KEY": {
          "description": "Amazon S3 Key",
          "value": ""
        },
        "S3_SECRET": {
          "description": "Amazon S3 Secret",
          "value": ""
        },
        "S3_REGION": {
          "description": "Amazon S3 Region",
          "value": "us-west-2"
        },
        "S3_BUCKET": {
          "description": "Amazon S3 Bucket Name",
          "value": ""
        },
        "APP_KEY": {
            "description": "Random 32 character string. Heroku does not have a generator for 32 character string ie: 22gauqhozv6fs6d3qpjy9bztf5qgjkir",
            "value": ""
        },
        "GOOGLE_CLIENT_ID": {
            "description": "Google Auth Client ID",
            "value": ""
        },
        "GOOGLE_CLIENT_SECRET": {
            "description": "Google Auth Secret",
            "value": ""
        },
        "GOOGLE_REDIRECT": {
            "description": "Google Auth redirect",
            "value": "http://localhost:8000/oauth2callback"
        }

    },
    "buildpacks": [
        {
            "url": "heroku/php"
        },
        {
            "url": "heroku/nodejs"
        }
    ],
    "engines": {
        "node": "5.10.1"
    }
}